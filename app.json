{
  "expo": {
    "name": "paywise",
    "slug": "paywise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-insights/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/app-insights/spash1.jpg",
      "resizeMode": "contain",      
      "backgroundColor": "#426ff5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-insights/logo.png",
        "backgroundColor": "#426ff5"
      }
    },
    "web": {      
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/app-insights/logo.png"
    },
    "plugins": [
      "expo-router",
      "@logrocket/react-native"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/paywise"
    }
  }
}